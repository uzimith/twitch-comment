{
    "compilerOptions": {
        "removeComments": false,
        "preserveConstEnums": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "experimentalDecorators": true,
        "outDir": "../dist/",
        "jsx": "react",
        "moduleResolution": "node",
        "module": "es6",
        "target": "es5",
        "baseUrl": ".",
        "lib": [
            "es5",
            "es2015.iterable",
            "es2015.collection",
            "es2015.promise",
            "dom"
        ],
        "typeRoots": ["./typings"]
    },
    "exclude": [
        "../node_modules",
        "../dist"
    ]
}
