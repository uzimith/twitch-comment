    "scripts": {
        "start": "electron ./app",
        "test": "npm-run-all -s test:build test:ava",
        "test:build": "webpack --config webpack.test.config.ts",
        "test:ava": "ava dist/tests.js",
        "build": "npm-run-all build:tcm build:webpack -s build:electron",
        "watch": "npm-run-all -p watch:webpack watch:tcm",
        "reload": "npm-run-all -p watch:electron watch:tcm",
        "start:debug": "webpack --env.debug",
        "build:tcm": "tcm src",
        "build:webpack": "webpack",
        "build:electron": "npm build.js",
        "watch:webpack": "webpack --watch --env.debug",
        "watch:tcm": "tcm -w src",
        "watch:electron": "webpack --watch --env.debug --env.electron",
        "watch:test": "npm-run-all -p watch:test:*",
        "watch:test:build": "webpack --watch --config webpack.test.config.ts",
        "watch:test:ava": "ava --watch dist/tests.js",
        "storybook": "npm-run-all -p storybook:*",
        "storybook:start": "start-storybook -p 9001",
        "storybook:open": "open http://localhost:9001",
        "storyshots": "storyshots",
        "push": "npm-run-all -p push:*",
        "push:git": "git push",
        "push:storybook": "storybook-to-ghpages"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uzimith/twitch-comment.git"
